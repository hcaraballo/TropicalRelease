              </xs:element>
              <xs:element name="ContainerBL" msdata:Locale="es-DO" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="BLNo" type="xs:string" minOccurs="1"/>
                    <xs:element name="ContainerNo" type="xs:string" minOccurs="1"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
