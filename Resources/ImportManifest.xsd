    <xs:complexType>
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:element name="Manifest" msdata:Locale="es-DO">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="AreaCode" type="xs:string" minOccurs="1"/>
              <xs:element name="TransportType" type="xs:string" minOccurs="1"/>
              <xs:element name="BizCompanyCode" type="xs:string" minOccurs="1"/>
              <xs:element name="VesselCode" type="xs:string" minOccurs="1"/>
