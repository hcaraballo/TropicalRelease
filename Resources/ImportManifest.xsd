                    <xs:element name="ConsignorEmail" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsignorFax" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsignorZipCode" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsignorStreet" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsignorZoneName" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsignorCity" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeType" type="xs:string" minOccurs="1" />
                    <xs:element name="ConsigneeCode" type="xs:string" minOccurs="1" />
                    <xs:element name="ConsigneeTel" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeCountryCode" type="xs:string" minOccurs="1"/>
                    <xs:element name="ConsigneeName" type="xs:string" minOccurs="0"/>
                    <xs:element name="ConsigneeDocumentType" type="xs:string" minOccurs="1"/>
