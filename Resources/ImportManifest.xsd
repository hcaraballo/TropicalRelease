                    <xs:element name="ConsigneeFax" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeZipCode" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeStreet" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeZoneName" type="xs:string" minOccurs="0" />
                    <xs:element name="ConsigneeCity" type="xs:string" minOccurs="0" />
