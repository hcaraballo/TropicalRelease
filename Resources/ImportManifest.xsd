			xmlns:mstns="http://aduanas.gob.do/XSD/ImportCargo/ImportManifest.xsd" 
			xmlns="http://aduanas.gob.do/XSD/ImportCargo/ImportManifest.xsd" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
			>
