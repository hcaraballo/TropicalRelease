                    <xs:element name="GrossWeight" type="xs:decimal" minOccurs="1"/>
                    <xs:element name="Value" type="xs:decimal" minOccurs="0" />
                    <xs:element name="FlightCharge" type="xs:decimal" minOccurs="0" />
                    <xs:element name="Volume" type="xs:decimal" minOccurs="0" />
