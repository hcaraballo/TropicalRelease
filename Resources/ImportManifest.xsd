                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="BLNo" type="xs:string" minOccurs="1"/>
                          <xs:element name="ChassisNo" type="xs:string" minOccurs="1"/>
                          <xs:element name="GrossWeight" type="xs:decimal" minOccurs="1"/>
